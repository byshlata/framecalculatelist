{
  "name": "framecalculatelist",
  "version": "0.0.0",
  "homepage": "https://byshlata.github.io/framecalculatelist",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext \".ts,.tsx\" \"**/*.{ts,tsx,*.ts,*tsx}\"",
    "lint:fix": "eslint --fix --ext \".ts,.tsx\" \"**/*.{ts,tsx,*.ts,*tsx}\"",
    "prepare": "husky install",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
